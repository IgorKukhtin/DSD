     MemoTare.Font.Size := 14;&#13;&#10;               MemoTare.Text := '[frxDBDHeader.&#34;NumPack&#34;] | [FormatDateTime( ''dd.mm'', &#60;frxDBDHeader.&#34;DatePack&#34;&#62;)]  |  [frxDBDHeader.&#34;NumTech&#34;] | [FormatDateTime( ''dd.mm'', &#60;frxDBDHeader.&#34;DateProduction&#34;&#62;)] ';&#13;&#10;            end      &#13;&#10;       else begin&#13;&#10;               // ТОЛЬКО дата для тары                                                                                                                                           &#13;&#10;               MemoTare.Font.Size := 26;&#13;&#10;               MemoTare.Text := '[FormatDateTime( ''dd.mm.yy'', &#60;frxDBDHeader.&#34;DateTare&#34;&#62;)]';&#13;&#10;            end;  &#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." PropData="0844617461736574730101061B20446174615365744E616D653D22667278444244486561646572220000095661726961626C65730101061B204E616D653D2269734A5047222056616C75653D2246616C7365220000055374796C650100">
