utVat  ;  &#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData2OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;   vbNDS := FormatFloat('0.##',&#60;NDS&#62;) + '%';                 &#13;&#10;     if &#60;frxDBDMaster.&#34;invnumbersp&#34;&#62; &#60;&#62; text&#13;&#10;      then&#13;&#10;         begin&#13;&#10;              text:= &#60;frxDBDMaster.&#34;invnumbersp&#34;&#62;;&#13;&#10;              CountR:= CountR + 1;&#13;&#10;         end;&#13;&#10;   if (&#60;frxDBDMaster.&#34;isprintlast&#34;&#62; = true) and (vbNewPage = 0)&#13;&#10;   then&#13;&#10;      begin&#13;&#10;           Engine.NewPage;&#13;&#10;           vbNewPage := 1;&#13;&#10;      end;  &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;{if (&#60;GroupMemberSPName&#62; = 'Дитячий') AND (&#60;isGroupMemberSP&#62; = FALSE)                      &#13;&#10;       then  ContractNumber := 5   &#13;&#10;       else  ContractNumber := 4;&#13;&#10;}&#13;&#10;    CountR:= 0;&#13;&#10;    text  := '';&#13;&#10;    vbNewPage := 0;     &#13;&#10;      &#13;&#10;    Page1.Visible := &#60;frxDBDMaster.&#34;hospitalid&#34;&#62; &#60;&#62; 4474306;  // &#34;Комунальний заклад &#34;ДЦПМСД №3&#34;&#34;            &#13;&#10;    Page2.Visible := &#60;frxDBDMaster.&#34;hospitalid&#34;&#62; = 4474306;        &#13;&#10;end." PropData="">
