                  Add('order by inJuridicalId, ObjectName, ObjectId');

        Open;
        cbPartnerBranchNal.Caption:='3.3. ('+IntToStr(RecordCount)+') Контрагенты Филиал-НАЛ';
        //
        fStop:=cbOnlyOpen.Checked;
        if cbOnlyOpen.Checked then exit;
