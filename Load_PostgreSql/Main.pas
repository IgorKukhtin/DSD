        toStoredProc.Params.AddParam ('inGoodsKindId',ftInteger,ptInput, 0);
        toStoredProc.Params.AddParam ('inAssetId',ftInteger,ptInput, 0);
        toStoredProc.Params.AddParam ('inPartionGoodsId',ftInteger,ptInput, 0);
        //
