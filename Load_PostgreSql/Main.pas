             toStoredProc.Params.ParamByName('inAssetId').Value:=FieldByName('AssetId_Postgres').AsInteger;
             toStoredProc.Params.ParamByName('inPartionGoodsId').Value:=PartionGoodsId_pg;
             if not myExecToStoredProc then ;//exit;
