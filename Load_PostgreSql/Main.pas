        Clear;
        Add('select Bill.Id as ObjectId');
        Add('     , Bill.BillDate as OperDate');
        Add('     , cast (Bill.BillNumber as integer) as InvNumber');
        Add('     , Bill.MoneyKindId');
        Add('     , zc_mkBN() as zc_mkBN');
        Add('     , UnitFrom.UnitName, UnitTo.UnitName');
