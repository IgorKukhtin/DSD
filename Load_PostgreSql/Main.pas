                  Add('     , null AS inPersonalTakeId');
                  Add('     , _pgPartner_find.PartnerId_pg as Id_Postgres');
                  Add('from (select min (_pgPartner.Id) as Id'
                    + '           , max (isnull(_pgPartner.PartnerId_pg,0)) as PartnerId_pg'
                    + '           , JuridicalId_pg'
                    + '           , OKPO'
                    + '           , Main'
                    + '      from dba._pgPartner'
