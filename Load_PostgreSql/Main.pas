        then
             Add(' and Bill.BillNumber = '+trim(OKPOEdit.Text))
        else

        if (cbOKPO.Checked)and (trim(OKPOEdit.Text)<>'') then
        begin
             Add(' and isnull (Information1.OKPO, Information2.OKPO)=' + FormatToVarCharServer_notNULL(trim(OKPOEdit.Text)));
